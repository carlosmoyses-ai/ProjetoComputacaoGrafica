// ---------------------------------------------------------------------------

#pragma hdrstop

#include "uJanela.h"
// ---------------------------------------------------------------------------
#pragma package(smart_init)

Janela::Janela(int winXmin, int winYmin, int winXmax, int winYmax)
{
	Xmin = winXmin;
	Ymin = winYmin;
	Xmax = winXmax;
	Ymax = winYmax;
}
